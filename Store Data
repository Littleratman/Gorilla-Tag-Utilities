using System;using System.Security.Cryptography;class Program{static void Main(string[] args){Random rnd=new Random();byte[] bytes=new byte[4000];rnd.NextBytes(bytes);string hexString="";foreach(byte b in bytes){hexString+=b.ToString("X2");}byte[] hash=MD5.Create().ComputeHash(System.Text.Encoding.UTF8.GetBytes(hexString));foreach(byte b in hash){Console.Write(b.ToString("X2"));}}}
